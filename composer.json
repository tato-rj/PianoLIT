{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "algolia/scout-extended": "^1.8",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.4",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^3.4",
        "laravel/scout": "^7.2",
        "laravel/tinker": "^1.0",
        "pixelpeter/laravel5-genderize-api-client": "^2.0",
        "predis/predis": "^1.1",
        "stevebauman/purify": "^3.0",
        "stripe/stripe-php": "^7.31",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "torann/geoip": "^1.0",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^3.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/homestead": "^8.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["support/helpers.php"],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/Support/helpers.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
